{
  "expo": {
    "name": "musicPlayer",
    "slug": "musicPlayer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "musicplayer",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.music.player",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.anonymous.musicPlayer",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "11204d1c-bd9b-4baa-bcd4-e8847f6e2183"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/11204d1c-bd9b-4baa-bcd4-e8847f6e2183"
    }
  }
}
